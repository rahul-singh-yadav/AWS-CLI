{
    "pipeline": {
        "roleArn": "arn:aws:iam:<account_id>:role/service-role/<code_pipeline_service_role>",
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "name": "From-Codecommit",
                        "actionTypeId": {
                            "category": "Source",
                            "owner": "AWS",
                            "version": "1",
                            "provider": "Codecommit"
                        },
                        "inputArtifacts": [],
                        "outputArtifacts": [
                            {
                                "name": "<SourceArtifacts>"
                            }
                        ],
                        "configuration": {
                            "RepositoryName": "SourceArtifacts",
                            "BranchName": "main",
                            "PollForSourceChanges": "false"

                        },
                        "runOrder": 1
                    }
                ]
            },
            {
                "name": "Build",
                "actions": [
                    {
                        "name": "At-Codebuild",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "AWS",
                            "version": "1",
                            "provider": "Codebuild"
                        },
                        "inputArtifacts": [{"name": "<SourceArtifacts>"}],
                        "outputArtifacts": [{"name": "<BuildArtifacts>"}],
                        "configuration": {
                            "PrimarySource": "<SourceArtifacts>",
                            "ProjectName": "<codebuild_project_name>"
                        },
                        "runOrder": 1
                    }
                ]
            },
            {
                "name": "Test",
                "actions": [
                    {
                        "name": "At-EC2",
                        "actionTypeId": {
                            "category": "Deploy",
                            "owner": "AWS",
                            "version": "1",
                            "provider": "Codedeploy"
                        },
                        "inputArtifacts": [{"name": "<BuildArtifacts>"}],
                        "outputArtifacts": [{"name": "<TestingArtifacts>"}],
                        "configuration": {
                            "ApplicationName": "<codedeploy_app_name>",
                            "DeploymentGroup": "<codedeploy_deployment_group>"
                        },
                        "runOrder": 1
                    }
                ]
            }
        ],
        "artifactStore": {
            "type": "S3",
            "location": "<bucket_name>-<current_region>"
        },
        "name": "three-stage-skeleton",
        "version": "1"
    },
    "tags": [
        {
            "key": "Env",
            "value": "Test"
        }
    ]
}