{
    "pipeline": {
        "roleArn": "arn:aws:iam:<account_id>:role/service-role/<code_pipeline_service_role>",
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "name": "From-Codecommit",
                        "actionTypeId": {
                            "category": "Source",
                            "owner": "AWS",
                            "version": "1",
                            "provider": "Codecommit"
                        },
                        "inputArtifacts": [],
                        "outputArtifacts": [
                            {
                                "name": "<SourceArtifacts>"
                            }
                        ],
                        "configuration": {
                            "RepositoryName": "SourceArtifacts",
                            "BranchName": "main",
                            "PollForSourceChanges": "false"

                        },
                        "runOrder": 1
                    }
                ]
            }
        ],
        "artifactStore": {},
        "name": "three-stage-skeleton",
        "version": "1"
    },
    "tags": [
        {
            "key": "Env",
            "value": "Dev"
        }
    ]
}