{
    "pipeline": {
        "roleArn": "arn:aws:iam::<account_id>:role/service-role/cfn-CodePipelineServiceRole",  
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "inputArtifacts": [],
                        "name": "Codecommit",
                        "actionTypeId": {
                            "category": "Source",
                            "owner": "AWS",
                            "version": "1",
                            "provider": "CodeCommit"
                        },
                        "outputArtifacts": [
                            {
                                "name": "SourceArtifacts"
                            }
                        ],
                        "configuration": {
                            "RepositoryName": "ObservabilityStack",
                            "BranchName": "gpl-ecs",
                            "PollForSourceChanges": "false"
                        },
                        "runOrder": 1                      
                    }
                ]
            },
            {
                "name": "Build",
                "actions": [
                    {
                        "name": "BuildCadvisor",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "AWS",
                            "version": "1",
                            "provider": "CodeBuild"
                        },
                        "configuration": {
                            "PrimarySource": "SourceArtifacts",
                            "ProjectName": "test-build-project"
                        },
                        "inputArtifacts": [
                            {
                                "name": "SourceArtifacts"
                            }
                        ],
                        "outputArtifacts": [
                            {
                                "name": "BuildArtifacts"
                            }
                        ],
                        "runOrder": 1
                    }
                ]
            }
        ],
        "artifactStore": {
            "type": "S3",
            "location": "codepipeline-ap-south-1-212316399146"
        },
        "name": "two-stage",
        "version": 1
    },
    "tags": [{
      "key": "Env",
      "value": "Test"
    }]
}